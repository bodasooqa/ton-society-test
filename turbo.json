{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "persistent": true,
      "cache": false
    },
    "preview": {
      "dependsOn": ["^preview"],
      "persistent": true,
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
